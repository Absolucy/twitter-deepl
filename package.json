{
	"name": "twitter-deepl",
	"version": "0.1.1",
	"displayName": "Twitter-DeepL",
	"author": "Lucy <lucy@absolucy.moe>",
	"description": "Use DeepL translation on twitter!",
	"scripts": {
		"build": "tsc && vite build",
		"watch": "vite build --watch --minify false",
		"dev": "vite",
		"serve:firefox": "web-ext run --start-url \"about:debugging#/runtime/this-firefox\"  --source-dir ./dist/",
		"serve:chrome": "web-ext run -t chromium --start-url \"https://google.com\" --source-dir ./dist/"
	},
	"license": "Zlib",
	"devDependencies": {
		"@samrum/vite-plugin-web-extension": "^0.1.1",
		"@types/firefox-webext-browser": "^94.0.1",
		"@types/webextension-polyfill": "^0.8.2",
		"typescript": "^4.5.5",
		"vite": "~2.8.0",
		"web-ext": "^6.6.0"
	},
	"dependencies": {
		"chrome-extension-async": "^3.4.1",
		"webextension-polyfill": "^0.8.0"
	}
}
